{
    "version": 2,
    "builds": [
        {
            "src": "insta_project/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb",
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "insta_project/wsgi.py"
        }
    ],
    "build": {
        "env": {
            "PYTHONUNBUFFERED": "true"
        }
    }
}